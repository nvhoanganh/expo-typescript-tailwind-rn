{
  "expo": {
    "name": "React native tailwind hook form",
    "description": "Showcase of an Expo app coded in TypeScript with Tailwind CSS.",
    "slug": "expo-and-typescript",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.0.0",
    "orientation": "portrait",
    "primaryColor": "#fff",
    "icon": "./assets/app-icon.png",
    "splash": {
      "backgroundColor": "#fff",
      "image": "./assets/splash-screen.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "RNPOC.Ios"
    },
    "githubUrl": "https://github.com/jongan69/expo-typescript-tailwind-rn",
    "android": {
      "package": "RNPOC.Android"
    }
  }
}
